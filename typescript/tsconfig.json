{
  "compilerOptions": {
    "baseUrl": ".",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "paths": {
      "@utils/*": [
        "./src/utils/*"
      ],
      "@data/*": [
        "./src/data/*"
      ],
    },
    "strict": true,
    "skipLibCheck": true,
    "target": "ES6",
  }
}
